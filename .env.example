# ============================================================
# Environment Variables for Feature Store
# Copy to .env and modify as needed
# ============================================================

# PostgreSQL Configuration
POSTGRES_USER=feast
POSTGRES_PASSWORD=feast_password
POSTGRES_DB=feast_registry
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
KAFKA_ZOOKEEPER_CONNECT=zookeeper:2181

# Spark Configuration
SPARK_MASTER_URL=spark://spark-master:7077

# Jupyter Configuration
JUPYTER_TOKEN=feast-token

# Airflow Configuration (if using standalone Airflow)
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://feast:feast_password@postgres:5432/airflow
AIRFLOW__CORE__FERNET_KEY=your-fernet-key-here
AIRFLOW__WEBSERVER__SECRET_KEY=your-secret-key-here
